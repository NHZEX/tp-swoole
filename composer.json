{
    "name": "nhzex/tp-swoole",
    "type": "library",
    "license": "Apache-2.0",
    "description": "",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2.10",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-swoole": "^4.3.2",
        "ext-curl": "*",
        "ext-sockets": "*",
        "open-smf/connection-pool": "~1.0",
        "topthink/framework": "6.0.x-dev",
        "topthink/think-helper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "swoole/ide-helper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\TpSwoole\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "HZEX\\TpSwoole\\Service"
            ],
            "config": {
                "swoole": "src/Config/swoole.php"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
