{
    "name": "nhzex/tp-swoole",
    "type": "library",
    "license": "Apache-2.0",
    "description": "",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.2.20 || ~7.3.8",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-sockets": "*",
        "ext-swoole": "~4.4",
        "monolog/monolog": "^2.0",
        "nhzex/n-helper": "^1.1",
        "nhzex/zsw-core": "^1.1",
        "open-smf/connection-pool": "~1.0",
        "psr/log": "^1.1",
        "symfony/finder": "^4.3",
        "symfony/var-dumper": "^4.3",
        "topthink/framework": "6.0.x-dev#9744571904a008bff8567d5fc929ed161cad0f8b",
        "topthink/think-helper": "^3.0",
        "topthink/think-orm": "^2.0.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "swoft/swoole-ide-helper": "~4.4.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\TpSwoole\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "HZEX\\TpSwoole\\Service"
            ],
            "config": {
                "swoole": "src/Config/swoole.php"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
